<launch>
 <arg name="moveit_controller_manager" default="ur10_robotiq_moveit_controller_manager/ur10_robotiqMoveItControllerManager" />
 <param name="moveit_controller_manager" value="$(arg moveit_controller_manager)"/>
 <arg name="controller_manager_name" default="ur10_robotiq_controller_manager" />
 <param name="controller_manager_name" value="$(arg controller_manager_name)" />
 <arg name="use_controller_manager" default="true" />
 <param name="use_controller_manager" value="$(arg use_controller_manager)" />
 <rosparam file="$(find ur10_robotiq_gazebo)/controller/controllers.yaml"/>
</launch>
